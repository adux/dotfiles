URxvt.font: xft:InconsolataGo Nerd Font Mono:size=14
URxvt.boldFont: xft:InconsolataGo Nerd Font Mono:size=14
URxvt.depth: 32
URxvt.borderless: true
URxvt.externalBorder: 0
URxvt.internalBorder: 0
URxvt.scrollBar: false
URxvt.saveLines: 65535
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true

!Tabbedex
!autohide when only 1 tab
URxvt.tabbed.autohide: true
!newbutton
URxvt.tabbed.new-button:false
! Meta modifier for keybindings
!URxvt.modifier: super

!! Perl Extensions
URxvt.perl-ext:             default,url-select,clipboard
URxvt.perl-ext-common:

! url-select (part of urxvt-perls package)
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.url-select.autocopy:  true
URxvt.url-select.button:    2
URxvt.url-select.launcher:  /usr/bin/firefox
URxvt.url-select.underline: true

! Nastavuje kopirovani
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy

! disable the stupid ctrl+shift 'feature'
URxvt.iso14755:     false
URxvt.iso14755_52:  false


